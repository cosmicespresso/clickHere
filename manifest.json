{
  "manifest_version": 2,
  "description": "attentional map",
  "name": "clickhere Parsons ",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
  	"scripts": ["background.js"]
  },
  "browser_action": {
	  "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "*.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ]
}
